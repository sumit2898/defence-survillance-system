    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@vercel/node": "^5.6.2",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.8",
    "postcss": "^8.5.6",
    "supertest": "^7.2.2",
    "tailwindcss": "^3.4.19",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
