{
  "name": "autonomous-shield",
  "version": "1.0.0",
  "description": "AI-powered surveillance system with real-time threat detection",
  "type": "module",
  "scripts": {
    "dev": "npx tsx server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "npx esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outfile=dist/index.js",
    "start": "npm run db:migrate && cross-env NODE_ENV=production node dist/index.js",
    "start:ai": "cd ai-service && python continuous_simulation.py",
    "start:full": "concurrently \"npm run dev\" \"npm run start:ai\"",
    "start:prod": "concurrently \"npm start\" \"npm run start:ai\"",
    "db:migrate": "drizzle-kit migrate",
    "test": "vitest run",
    "test:db": "npx tsx scripts/test_db_connection.ts",
    "test:breach": "npx tsx scripts/test_breach.ts",
    "test:realtime": "npx tsx scripts/test_realtime.ts",
    "verify:rls": "npx tsx scripts/verify_rls.ts",
    "test:audit": "npx tsx scripts/test_audit.ts"
  },
  "keywords": [
    "surveillance",
    "ai",
    "yolov8",
    "threat-detection",
    "security",
    "automation"
  ],
  "author": "HackOps Crew",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@replit/vite-plugin-runtime-error-modal": "^0.0.4",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-virtual": "^3.13.18",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.1",
    "@vitejs/plugin-react": "^5.1.3",
    "axios": "^1.13.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "compression": "^1.8.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.4",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "express": "^5.2.1",
    "framer-motion": "^12.33.0",
    "helmet": "^8.1.0",
    "input-otp": "^1.4.2",
    "leaflet": "^1.9.4",
    "leaflet.heat": "^0.2.0",
    "lenis": "^1.3.17",
    "lucide-react": "^0.563.0",
    "nanoid": "^5.1.6",
    "next-themes": "^0.4.6",
    "pg": "^8.18.0",
    "postgres": "^3.4.8",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "react-leaflet": "^5.0.0",
    "react-virtualized-auto-sizer": "^2.0.2",
    "react-window": "^2.2.6",
    "recharts": "^3.7.0",
    "tailwind-merge": "^3.4.0",
    "tsx": "^4.21.0",
    "vaul": "^1.1.2",
    "vite": "^7.3.1",
    "wouter": "^3.9.0",
    "ws": "^8.19.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
